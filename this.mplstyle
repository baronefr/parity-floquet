# minimal plot styling for this thesis

figure.figsize : 6, 4
font.size: 12.0
font.family: sans-serif
#font.serif: Palatino
font.sans-serif: Helvetica
axes.titlesize: medium
figure.titlesize: medium
text.usetex: True
text.latex.preamble: \usepackage{amsmath}\usepackage{amssymb}
xtick.direction: in
xtick.major.size: 5
ytick.direction: in
ytick.major.size: 5
xtick.top: True
ytick.right: True
legend.frameon: False
